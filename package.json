		"eslint": "^9.17.0",
		"globals": "^15.14.0",
		"harperdb": "^4.7.19",
		"next": "^15.5.12",
		"prettier": "^3.4.2",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"next": "^15.5.12 || ^16.1.6"
