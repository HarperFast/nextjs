{
	"name": "@harperfast/nextjs",
	"version": "1.1.4",
	"type": "module",
	"description": "A Harper plugin for running Next.js apps.",
	"keywords": [
		"harperdb",
		"next.js",
		"distributed",
		"applications"
	],
	"homepage": "https://github.com/HarperFast/nextjs",
	"bugs": {
		"url": "https://github.com/HarperFast/nextjs/issues",
		"email": "support@harperdb.io"
	},
	"author": {
		"name": "HarperDB",
		"email": "support@harperdb.io"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HarperFast/nextjs.git"
	},
	"license": "Apache-2.0",
	"bin": {
